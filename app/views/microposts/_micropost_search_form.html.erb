<%= search_form_for @q ,url: user_path do |f| %>
  <%= f.label :content_cont, '投稿検索' %>
  <div class="input-group">
    <%= f.text_field :content_cont, placeholder: "検索したい投稿を入力してください。",
                      class: 'form-control' %>
    <span class="input-group-btn">
     <%= f.submit '検索', class: "btn btn-primary" %>
    </span>
  </div>
<% end %>